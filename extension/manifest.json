{
  "manifest_version": 3,
  "name": "DPBH Extension",
  "version": "2.0",
  "description": "This extension is for the DPBH hackathon",
  "permissions": [
    "activeTab","tabs", "scripting"
  ],
  "background": {
    "service_worker": "/scripts/background.js"
  },
  "action": {
    "default_popup": "/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["./scripts/content.js"]
    }
  ]
}
